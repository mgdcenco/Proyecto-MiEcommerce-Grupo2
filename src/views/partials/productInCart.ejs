<article class="product-in-cart">
  <div class="product-img">
    <img
      src="<%= product.images[product.images.length-1] %>"
      alt="<%= product.title %>"
    />
  </div>
  <div class="product-details">
    <div><p><%= product.title %></p></div>
    <div class="cart-actions">
      <div>
        <button class="section-product-detail__button">Quitar</button>
        <div class="qty-selector">
          <button class="qty-selector-btn">-</button><span>1</span
          ><button class="qty-selector-btn">+</button>
        </div>
      </div>
      <span class="product-price"><%= product.price %></span>
    </div>
  </div>
</article>
